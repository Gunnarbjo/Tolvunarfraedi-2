import edu.princeton.cs.algs4.*;

public class Testla{
  public static void main(String[] args){
    int N = Integer.parseInt(args[0]);

    Stopwatch st = new Stopwatch();
    int sum = 0;
    int reps 1000;

    for(int k = 0; k < reps; k++){
      //breyta Ã¾essu
      sum = 0+k;
      for (int n = N; n > 0; n/=2){
      int s = 0;
        for(int i = 0; i < n; i++){
          s++;
          }
          sum += s;
        }
      }
      double t = s.elapsedTime(;);

      StdOut.println(t);
    }
  }
